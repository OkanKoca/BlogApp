@model List<Post>

<div class="card mb-3">
	<div class="card-header">
		<h5 class="card-title">Recent Posts</h5>
	</div>
	<div class="card-body">
		@foreach(var post in Model)
		{
			<div class="mb-3">
				<a asp-action="Details" asp-route-url="@post.Url">
					<h6 class="mb-6">@post.Title</h6>
				</a>
			</div>
		}
	</div>
</div>